<% if !ele.nil? && ele.errors.any? %>
	<div id="url-error" class="error-alert">
	    <h2 class="alert-headline">Woops! <%= pluralize(ele.errors.count, "error") %>:</h2>
	    <ul class="alert-bullets">
	    <% ele.errors.full_messages.each do |msg| %>
	      <li><%= msg %></li>
    	<% end %>
    	</ul>
  	</div>
<% end %>